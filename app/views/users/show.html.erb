<div class="container">
  <div class="header">
    <%= link_to 'Back', :back, class:"btn btn-primary" %>
    <%= image_tag "https://images.unsplash.com/photo-1562925898-efd092faeff0?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60", class: 'avatar' %>
    <h1><%= @student.first_name %></h1>
  </div>
  <div class="analytics">
    <%= month_calendar do |date| %>
      <%= date.day %>
    <% end %>
    <br>
    <%= line_chart [[@student.emotions.first.created_at, @student.emotions.first.rating], ["b", 2], ["c", 3], ["d", 1]], label: "SOMETHING",xtitle: "Date", ytitle: "Emotions" %>
  </div>
  </div>
</div>
